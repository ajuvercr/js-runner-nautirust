{
    "id": "JsRunner",
    "start": "npm install && npm run build",
    "docker": "node ./docker.js {config} {cwd}",
    "runnerScript": "node ./lib/index.js {config} {cwd}",
    "canUseChannel": [
        "file",
        "ws",
        "kafka"
    ],
    "canUseSerialization": [
        "json",
        "jsonld",
        "turtle",
        "trig",
        "n-triples",
        "n-quads",
        "n3",
        "plain",
        "xml"
    ],
    "requiredFields": [
        "jsFile",
        "methodName"
    ]
}
